generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model CatalogItem {
  id        Int      @id @default(autoincrement())
  image     String   @unique
  name      String
  type      String?
  material  String?
  /// Lista de ambientes associados ao item (JSON serializado).
  ambientes String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
